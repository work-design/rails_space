<div data-controller="scan">
  <div class="buttons are-large is-centered">
    <button class="button is-success" data-action="scan#report" data-report-url="<%= url_for(controller: 'space/me/grids', action: 'in', room_id: @grid.room_id, id: @grid.id) %>">入库产品</button>
    <button class="button is-info" data-action="scan#report" data-report-url="<%= url_for(controller: 'space/me/grids', action: 'out', room_id: @grid.room_id, id: @grid.id) %>">出库产品</button>
  </div>
</div>
